var n_altitudes               = 2;
var altitude_colors           = [ 'orange', 'blue' ];
var default_metric_altitudes  = [ 3000, 10000 ];
var default_english_altitudes = [ 10000 * METERS_PER_FOOT, 30000 * METERS_PER_FOOT ];
var upintheair_overlays;
function altset(i) { return document.f_settings['altitude' + i]; }

function init_upintheair() {
  var a = settings.use_metric? default_metric_altitudes : default_english_altitudes;
  for (var i = 0; i < n_altitudes; i++)
    altset(i).value = round0(meters_to_units(a[i]));
}


function show_upintheair() {
  remove_upintheair();
  var sent = [];
  var s    = '';

  for (var i = 0; i < n_altitudes; i++) {
    altset(i).style.display = 'inline';
    var alt = units_to_meters(altset(i).value - 0);
    if (altset(i).value != '' && alt < result.elev_amsl) {
      alert(altitude_colors[i] + ' altitude too low');
      continue;
    }
    sent.push(i);
    s += ',' + alt;
  }
  s = s.substr(1);

  wt_async_request('/bin/upintheair.json?id=' + result.id + '&alts=' + s, 'UpInTheAir API', function(s) {
    upintheair_overlays = [];
    var a = JSON.parse(s).rings;
    for (var j = 0; j < sent.length; j++) {
      var i = sent[j];
      var lines = [];
      a[i].points._foreach(function(p) { lines.push({ lat: p[0], lng: p[1] }); });
      upintheair_overlays.push(new $m.Polyline({ map: map, path: lines, strokeColor: altitude_colors[i], strokeWeight: 2, strokeOpacity: 1, clickable: false }));
    }
  });
}
		  

function remove_upintheair() {
  for (var i = 0; i < n_altitudes; i++)
    altset(i).style.display = 'none';
  set_all_map(upintheair_overlays, null);
  upintheair_overlays = null;
}

function get_upintheair_kml() {
  let alts = '';
  let colors='';  
  for (let i = 0; i < n_altitudes; i++) {
    var alt = altset(i).value - 0;
    if (altset(i).value == '' || units_to_meters(alt) <  result.elev_amsl)
      continue;
    alts += ',' + alt;
    colors += ',' + (i == 0? 'ffa500' : '0000ff');
  }
  if (alts == '')
    return;
  location.href = 'bin/aircone.kml?src=main&id=' + result.id + '&alts=' + alts.substr(1) + '&colors=' + colors.substr(1) + '&ringsonly=1' + (settings.use_metric? '&metric=1' : '');
}

